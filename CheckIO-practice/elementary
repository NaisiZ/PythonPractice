#1 FizzBuzz

def checkio(number):
    #Your code here
    #It's main function. Don't remove this function
    #It's using for auto-testing and must return a result for check.
    if (number%3)==0 and (number%5)==0:
        number= ("Fizz Buzz")
    elif (number%5)==0:
        number= ("Buzz")
    elif (number%3)==0:
        number= ("Fizz")
    else:
        print (number)
    #replace this for solution
    return str(number)

#2 index_power

def index_power(array, n):
    if n==0:
        return (1)
    elif len(array) <= n:
        return (-1)
    else:
        return (array[n]**n)

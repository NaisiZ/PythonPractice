or k in range(1,n 1): # loop for k = 1, 2, 3, ..., n 
  stars = "" 
  for i in range(k): # loop k times, not caring about i 
     stars = stars "*" 
  print stars

 print "Q1 print an reversed asterist triangle"
 n=4
 while n>0:
         print "*"*n
         n=n-1
 print "Q2 print an asterist triangle"
 n=1
 while n<5:
         print "*"*n
         n=n+1
